<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxhan.modular.mapper.EnergyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zxhan.modular.entity.Energy">
        <id column="id" property="id" />
        <result column="room_id" property="roomId" />
        <result column="energy_type" property="energyType" />
        <result column="usage" property="usage" />
        <result column="pay_monry" property="payMonry" />
        <result column="pay" property="pay" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, room_id, energy_type, usage, pay_monry, pay
    </sql>

</mapper>
